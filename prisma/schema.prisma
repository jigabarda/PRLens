generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model PullRequest {
  id      Int     @id @default(autoincrement())
  title   String
  url     String  @unique
  state   String
  author  String
}

model RepoHistory {
  id        Int      @id @default(autoincrement())
  repoUrl   String
  owner     String
  repo      String
  totalPRs  Int
  createdAt DateTime @default(now())
}
